{
    "manifest_version": 3,
    "name": "Pf-extension",
    "description": "Performoo Extensions",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}